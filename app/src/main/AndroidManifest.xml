<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kiran.googlepluseintegration" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />
        <activity android:name=".thirdclass" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
<!--permissions
-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--
    retrive account name as part of sign in
    -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <!--
    retrieve OAuth 2.0 tokens or invalidate tokens to disconnect a user
    for disconnect purpose also
    -->
    <uses-permission android:name="android.permission.USE_CREDENTIALS"/>

</manifest>
